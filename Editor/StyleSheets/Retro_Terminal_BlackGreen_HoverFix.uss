:root {
    --font: resource("Fonts/JetBrainsMono-Regular");
    -unity-font-definition: var(--font) !important;
    /* --- Base & Global --- */
    --color-text-primary: #39ff14;
    --color-text-secondary: rgba(57, 255, 20, 0.65);
    --color-text-accent: rgb(170, 255, 170);
    --color-text-focus: #aaffaa;
    --color-accent-blue: #39ff14; /* Replaced with green */
    --color-accent-gold: rgb(170, 255, 170); /* Replaced with light green */
    --color-transparent: rgba(0, 0, 0, 0);

    /* --- Main Panels & Views --- */
    --panel-background-left: rgb(0, 0, 0);
    --panel-background-main: rgb(5, 15, 5);
    --panel-background-settings: rgb(0, 0, 0);
    --panel-background-viewport: rgb(0, 0, 0);
    --panel-background-group-container: rgb(10, 20, 10);
    --panel-background-box: rgba(10, 25, 10, 0.3);
    --panel-background-help-box: rgb(15, 30, 15);
    --panel-border-help-box: rgb(0, 50, 0);

    /* --- PlayMode Toolbar --- */
    --playmode-button-background-default: rgb(0, 60, 0);
    --playmode-button-background-hover: rgb(10, 80, 10);
    --playmode-button-background-active: rgb(26, 129, 26);

    /* --- Toolbar --- */
    --toolbar-background-default: rgb(10, 20, 10);
    --toolbar-background-main: rgb(0, 0, 0);
    --toolbar-border-default: rgb(0, 40, 0);
    --toolbar-breadcrumbs-text-color: var(--color-text-primary);
    --toolbar-overlay-background-default: rgb(10, 30, 10);
    --toolbar-overlay-background-hover: rgb(20, 50, 20);

    /* --- Toolbar Button --- */
    --toolbar-button-background-default: rgb(10, 20, 10);
    --toolbar-button-background-hover: rgb(20, 40, 20);
    --toolbar-button-background-active: rgb(30, 60, 30);
    --toolbar-button-border-default: rgb(0, 40, 0);
    --toolbar-button-border-focus: var(--color-text-primary);
    --toolbar-button-text-color: var(--color-text-primary);

    /* --- Toolbar Toggle --- */
    --toolbar-toggle-background-default: rgb(10, 20, 10);
    --toolbar-toggle-background-hover: rgb(20, 40, 20);
    --toolbar-toggle-background-active: rgb(30, 60, 30);
    --toolbar-toggle-background-checked: rgb(25, 80, 25);
    --toolbar-toggle-border-default: rgb(0, 40, 0);
    --toolbar-toggle-text-color: var(--color-text-primary);

    /* --- Toolbar Menu --- */
    --toolbar-menu-background-default: rgb(10, 20, 10);
    --toolbar-menu-background-hover: rgb(20, 40, 20);
    --toolbar-menu-background-active: rgb(30, 60, 30);
    --toolbar-menu-border-default: rgb(0, 40, 0);
    --toolbar-menu-text-color: var(--color-text-primary);

    /* --- Experimental Tag (Toolbar/Global) --- */
    --experimental-tag-background: rgb(20, 0, 0);
    --experimental-tag-border-color: var(--color-accent-gold);
    --experimental-tag-text-color: var(--color-accent-gold);

    /* --- Button --- */
    --button-background-default: rgb(0, 55, 0);
    --button-background-hover: rgb(10, 75, 10);
    --button-background-active: rgb(20, 95, 20);
    --button-border-color-top-default: rgb(15, 45, 15);
    --button-border-color-sides-default: rgb(15, 45, 15);
    --button-border-color-bottom-default: rgb(0, 30, 0);
    --button-border-color-top-active: rgb(0, 15, 0);
    --button-border-color-active: rgb(0, 30, 0);
    --button-border-color-focus: var(--color-text-primary);
    --button-text-color: var(--color-text-primary);

    /* --- Button Group --- */
    --button-group-background-active: rgb(20, 95, 20);
    --button-group-background-hover: rgb(15, 85, 15);
    --button-group-border-left-focus: rgb(15, 45, 15);
    --button-strip-field-background-checked: rgb(25, 105, 25);

    /* --- Base Text Field --- */
    --base-text-field-background-default: rgb(0, 0, 0);
    --base-text-field-border-top-default: rgb(0, 15, 0);
    --base-text-field-border-sides-default: rgb(0, 30, 0);
    --base-text-field-border-bottom-default: rgb(0, 30, 0);
    --base-text-field-border-color-hover: rgb(10, 70, 10);
    --base-text-field-border-color-focus: var(--color-text-primary);
    --base-text-field-text-color: var(--color-text-primary);
    --base-text-field-cursor-color: var(--color-text-focus);
    --base-text-field-selection-color: rgba(0, 80, 0, 0.65);

    /* --- Search Field --- */
    --search-field-background-default: rgb(0, 0, 0);
    --search-field-border-top-default: rgb(0, 15, 0);
    --search-field-border-sides-default: rgb(0, 30, 0);
    --search-field-border-bottom-default: rgb(0, 30, 0);
    --search-field-border-color-hover: rgb(10, 70, 10);
    --search-field-border-color-focus: var(--color-text-primary);

    /* --- Popup Field (Enum, etc.) --- */
    --popup-field-background-default: rgb(0, 55, 0);
    --popup-field-background-hover: rgb(10, 75, 10);
    --popup-field-border-color-top-default: rgb(15, 45, 15);
    --popup-field-border-color-sides-default: rgb(15, 45, 15);
    --popup-field-border-color-bottom-default: rgb(0, 30, 0);
    --popup-field-border-color-focus: var(--color-text-primary);
    --popup-field-text-color: var(--color-text-primary);

    /* --- Dropdown Menu --- */
    --dropdown-menu-background-outer: rgb(0, 0, 0);
    --dropdown-menu-background-inner: rgb(10, 20, 10);
    --dropdown-menu-background-item-hover: rgb(20, 50, 20);
    --dropdown-menu-border-color-outer: rgb(0, 30, 0);
    --dropdown-menu-border-color-inner: rgb(10, 70, 10);
    --dropdown-menu-separator-color: rgb(10, 70, 10);
    --dropdown-menu-text-color: var(--color-text-primary);

    /* --- Slider --- */
    --slider-background-tracker: rgb(0, 40, 0);
    --slider-background-dragger: var(--color-text-primary);
    --slider-background-dragger-hover: var(--color-text-focus);
    --slider-background-tracker-disabled: rgb(0, 20, 0);
    --slider-background-dragger-disabled: rgb(0, 60, 0);
    --slider-background-fill: var(--color-text-primary);
    --slider-background-fill-disabled: rgb(0, 60, 0);
    --slider-border-color-dragger: rgb(0, 60, 0);
    --slider-border-color-dragger-disabled: rgb(0, 40, 0);
    --slider-dragger-border-background: rgba(170, 255, 170, 0.5);

    /* --- MinMax Slider --- */
    --min-max-slider-background-tracker: rgb(0, 40, 0);
    --min-max-slider-background-thumb: var(--color-text-primary);
    --min-max-slider-background-thumb-hover: var(--color-text-focus);
    --min-max-slider-background-movable: var(--color-text-primary);
    --min-max-slider-border-color-thumb: rgb(0, 60, 0);
    --min-max-slider-border-color-movable: var(--color-text-focus);

    /* --- Radio Button --- */
    --radio-button-background-unchecked: rgb(0, 0, 0);
    --radio-button-background-checked: var(--color-text-primary);
    --radio-button-border-color-default: rgb(0, 60, 0);
    --radio-button-border-color-hover: rgb(10, 80, 10);
    --radio-button-border-color-focus: var(--color-text-primary);
    --radio-button-text-color-focus: var(--color-text-accent);

    /* --- Object Field --- */
    --object-field-background-default: rgb(0, 0, 0);
    --object-field-background-selector: rgb(0, 55, 0);
    --object-field-background-selector-hover: rgb(10, 75, 10);
    --object-field-background-accept-drop: rgb(20, 95, 20);
    --object-field-border-color-default: rgb(0, 50, 0);
    --object-field-border-color-hover: rgb(10, 80, 10);
    --object-field-border-color-focus: var(--color-text-primary);
    --object-field-text-color-default: var(--color-text-secondary);
    --object-field-text-color-focus: var(--color-text-primary);
    --object-field-display-text-color: var(--color-text-primary);

    /* --- Collection/List/Tree Views --- */
    --collection-view-background-item-alt: rgb(5, 20, 5);
    --collection-view-background-item-hover: rgb(10, 40, 10);
    --collection-view-background-item-selected: rgb(15, 60, 15);
    --collection-view-background-item-selected-focus: rgb(20, 80, 20);
    --collection-view-background-drag-hover: rgb(20, 50, 20);
    --collection-view-background-drag-hover-selected-focus: rgb(25, 90, 25);
    --collection-view-drag-bar-color: var(--color-text-focus);
    --collection-view-drag-marker-border-color: var(--color-text-focus);
    --collection-view-border-color: rgb(0, 40, 0);
    --collection-view-text-color-selected: var(--color-text-primary);
    --tree-view-background-default: rgb(0, 0, 0);
    --list-view-background-footer: rgb(10, 25, 10);
    --list-view-background-header: rgb(10, 20, 10);
    --list-view-background-header-hover: rgb(15, 35, 15);
    --list-view-background-reorder-handle: var(--color-text-secondary);
    --list-view-border-color-default: rgb(0, 40, 0);
    --list-view-border-color-header: rgb(0, 50, 0);

    /* --- Scroller --- */
    --scroller-background-default: rgba(0, 0, 0, 0.2);
    --scroller-background-button-hover: rgb(10, 40, 10);
    --scroller-background-dragger: rgb(15, 60, 15);
    --scroller-background-dragger-hover: rgb(20, 80, 20);
    --scroller-border-color-dragger: rgb(0, 40, 0);
    --scroller-border-color-vertical: rgba(0, 30, 0, 0.5);
    --scroller-border-color-horizontal: rgba(0, 30, 0, 0.5);

    /* --- Tabs --- */
    --tab-background-header-default: rgb(0, 0, 0);
    --tab-background-header-hover: rgb(10, 30, 10);
    --tab-background-header-underline: var(--color-text-primary);
    --tab-background-dragging: rgb(20, 50, 20);
    --tab-content-background: rgb(5, 15, 5);
    --tab-view-drag-preview-border-color: var(--color-text-primary);
    --tab-view-drag-location-preview-color: rgb(15, 60, 15);
    --tab-text-color: var(--color-text-primary);

    /* --- Progress Bar --- */
    --progress-bar-background: rgb(0, 30, 0);
    --progress-bar-background-fill: var(--color-text-primary);
    --progress-bar-border-color: rgb(0, 50, 0);
    --progress-bar-text-color: rgb(0,0,0);

    /* --- Color Field --- */
    --color-field-background-eyedropper: rgb(0, 40, 0);
    --color-field-background-eyedropper-hover: rgb(10, 60, 10);
    --color-field-background-mixed-value: rgb(20, 40, 20);
    --color-field-progress-bar-background: rgb(0, 0, 0);
    --color-field-progress-bar-fill: var(--color-text-primary);
    --color-field-border-color-default: rgb(0, 50, 0);
    --color-field-border-color-hover: rgb(10, 80, 10);
    --color-field-border-color-focus: var(--color-text-primary);
    --color-field-text-color-mixed-value: var(--color-text-primary);

    /* --- Curve Field --- */
    --curve-field-background: rgb(5, 25, 5);
    --curve-field-border-color-default: rgb(0, 50, 0);
    --curve-field-border-color-focus: var(--color-text-primary);
    --curve-field-curve-color: var(--color-text-primary);

    /* --- Gradient Field --- */
    --gradient-field-border-color-default: rgb(0, 50, 0);
    --gradient-field-border-color-focus: var(--color-text-primary);
    --gradient-field-inner-border-color-default: rgb(0, 30, 0);
    --gradient-field-inner-border-color-focus: var(--color-text-focus);

    /* --- Multi-Column Header --- */
    --multi-column-header-background-default: rgb(10, 25, 10);
    --multi-column-header-background-hover: rgb(15, 40, 15);
    --multi-column-header-background-active: rgb(20, 60, 20);
    --multi-column-header-background-moving: rgb(25, 80, 25);
    --multi-column-header-resize-preview-color: var(--color-text-primary);
    --multi-column-header-move-preview-border-color: var(--color-text-primary);
    --multi-column-header-border-right-default: rgb(0, 50, 0);

    /* --- Pop-up Window --- */
    --popup-window-background: rgba(5, 20, 5, 0.95);
    --popup-window-border-color: rgb(0, 50, 0);
    --popup-window-root-border-color: rgb(20, 80, 20);
    --popup-window-text-color: var(--color-text-primary);

    /* --- Binding (Prefab Overrides, Animation) --- */
    --binding-bar-background-prefab-override: rgb(0, 128, 128); /* Teal */
    --binding-bar-background-live-property: rgb(128, 128, 0); /* Olive */
    --binding-bar-background-preset-ignore: rgb(128, 0, 128); /* Purple */
    --binding-tint-color-animation-animated: rgb(0, 200, 200); /* Bright Cyan */
    --binding-tint-color-animation-recorded: rgb(255, 100, 0); /* Orange */
    --binding-tint-color-animation-candidate: rgb(255, 165, 0); /* Lighter Orange */

    /* --- Misc --- */
    --foldout-text-color-focus: var(--color-text-accent);
    --two-pane-split-view-dragline-color: var(--color-transparent);
    --two-pane-split-view-dragline-anchor-color: rgb(15, 60, 15);
    --composite-field-text-color-mixed-value: var(--color-text-secondary);
    --overlay-toolbutton-background-color: rgb(0, 40, 0);
}

